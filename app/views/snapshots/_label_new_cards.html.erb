<div id="label_new_cards" style='display: none'>
  <div class="progress"><div class="bar" style="width: 1%;"></div></div>
  <div class="well">
    <div>card found in the snapshot</div>
    <%= form_for [@wall, snapshot, Card.new], :remote => true, :html => { :method => 'PUT' } do |f| %>
      <%= f.hidden_field :positive, :value => '1' %>
      <div class="clearfix">
      <img src='/images/loading.gif' class='card'/>
      <div class="identifier">
        <%= f.label :identifier, "Card Identifier:" %>
        <%= f.text_field :identifier, :class => 'text_field typeahead',
                :placeholder => "A number/name identifiers the card",
                'data-source' => auto_complete_suggestion_cards.map(&:identifier).inspect,
                'data-source-img' => auto_complete_suggestion_cards.map(&:image_uri).inspect,
                'data-provide' => "typeahead" %>
      </div>
      <button class="btn btn-large icon-thumbs-down" type="button">
        Not a Card, Next >
      </button>
      </div>
      <div class="form-actions clearfix">
        <button class="btn btn-large icon-chevron-left prev-image" type="button">
          Prev
        </button>
        <button class="btn btn-large btn-primary icon-thumbs-up" type="submit">
          Is a Card, Next >
        </button>
      </div>
    <%- end -%>
  </div>
</div>
